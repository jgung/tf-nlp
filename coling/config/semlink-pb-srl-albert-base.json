{
  "reader": "./reader/semlink.json",
  "checkpoint_epochs": 0.5,
  "max_epochs": 3,
  "batch_size": 32,
  "optimizer": {
    "name": "bert",
    "lr": {
      "name": "bert",
      "rate": 0.00005,
      "warmup_proportion": 0.1
    },
    "clip": 1.0
  },
  "features": {
    "targets": [
      "./feats/srl.json"
    ],
    "inputs": [
      {
        "name": "bert",
        "drop_subtokens": true,
        "model": "https://tfhub.dev/google/albert_base/3"
      },
      "./feats/marker.json"
    ]
  },
  "encoders": [
    "./encoders/bert-no-subtokens.json",
    "./encoders/bert-dropout.json",
    "./encoders/srl-baseline-inputs.json",
    "./encoders/srl-blstm.json",
    "./encoders/srl-repeat-predicate.json",
    "./encoders/srl-concat-predicate.json",
    "./encoders/srl-mlp.json"
  ],
  "heads": [
    "./heads/pb-head.json"
  ]
}