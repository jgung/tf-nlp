{
  "reader": {
    "field_index_map": {
      "word": 3,
      "xpos": 4,
      "predicate": 6,
      "deprel": 8,
      "head": 9
    },
    "pred_start": 13,
    "map_with_regex": true,
    "label_mappings": {
      "gold": {
        "^(\\S+)\\$(\\S+)": "\\2"
      }
    }
  },
  "checkpoint_epochs": 0.25,
  "max_epochs": 4,
  "batch_size": 16,
  "optimizer": "./optim/bert.json",
  "features": {
    "targets": [
      "./feats/srl.json"
    ],
    "inputs": [
      {
        "name": "bert",
        "drop_subtokens": true,
        "model": "https://tfhub.dev/google/bert_cased_L-12_H-768_A-12/1",
        "seg_ids": "pred_marker"
      },
      "./feats/marker.json"
    ]
  },
  "encoders": [
    "./encoders/bert-no-subtokens.json",
    "./encoders/bert-dropout.json",
    "./encoders/bert-srl-input.json"
  ],
  "heads": [
    "./heads/pb-head.json"
  ]
}