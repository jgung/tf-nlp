{
  "reader": "conll_2009",
  "throttle_secs": 0,
  "checkpoint_steps": 1500,
  "state_size": 200,
  "dropout": 0.33,
  "input_dropout": 0.0,
  "mlp_dropout": 0.33,
  "encoder": "lstm",
  "encoder_layers": 3,
  "batch_size": 10,
  "crf": true,
  "optimizer": {
    "name": "Adam",
    "params": {
      "beta1": 0.9,
      "beta2": 0.999
    },
    "lr": {
      "rate": 0.002,
      "name": "exponential_decay",
      "params": {
        "decay_steps": 5000,
        "decay_rate": 0.75
      }
    },
    "clip": 5.0
  },
  "features": {
    "seq_feat": "word",
    "targets": [
      {
        "name": "head",
        "key": "head",
        "rank": 2,
        "numeric": true
      },
      {
        "name": "deprel",
        "key": "deprel",
        "rank": 2,
        "indices": {
          "O": 0
        },
        "unknown_word": "O"
      }
    ],
    "features": [
      {
        "name": "word",
        "key": "word",
        "rank": 2,
        "mapping_funcs": [
          "lower"
        ],
        "config": {
          "dim": 100,
          "trainable": true,
          "word_dropout": 0.33,
          "dropout": 0,
          "initializer": {
            "embedding": "vectors/glove.6B.100d.txt",
            "pkl_path": "word-vectors.pkl",
            "include_in_vocab": 100000
          },
          "func": {
          }
        }
      },
      {
        "name": "pos",
        "key": "ppos",
        "rank": 2,
        "config": {
          "dim": 100,
          "trainable": true,
          "word_dropout": 0.33,
          "dropout": 0
        }
      }
    ]
  }
}