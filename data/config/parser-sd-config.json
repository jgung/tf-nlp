  ],
  "metric": "LAS",
  "optimizer": {
    "name": "LazyAdam",
    "params": {
      "beta1": 0.9,
      "beta2": 0.9
    },
    "lr": 0.002,
    "clip": 5.0
  },
  "features": {
    "seq_feat": "head",
    "targets": [
      {
        "name": "head",
        "key": "head",
        "numeric": true,
        "padding_funcs": [
          {
            "type": "pad",
            "count": 1,
            "value": 0
          }
        ]
      },
      {
        "name": "deprel",
        "key": "deprel",
        "indices": {
          "O": 0
        },
        "unknown_word": "O",
        "padding_funcs": [
          {
            "type": "pad",
            "count": 1,
            "value": "<ROOT>"
          }
        ]
      }
    ],
    "inputs": [
      {
        "name": "fix_word",
        "key": "word",
        "mapping_funcs": [
          "lower"
        ],
        "padding_funcs": [
          {
            "type": "pad",
            "count": 1,
            "value": "<ROOT>"
          }
        ],
        "config": {
          "dim": 100,
          "trainable": false,
          "word_dropout": 0.33,
          "add_group": "words",
          "initializer": {
            "embedding": "vectors/glove.6B.100d.txt",
            "pkl_path": "word-vectors.pkl",
            "include_in_vocab": 400000,
            "zero_init": true,
            "restrict_vocab": true
          }
        }
      },
      {
        "name": "train_word",
        "key": "word",
        "threshold": 2,
        "mapping_funcs": [
          "lower"
        ],
        "padding_funcs": [
          {
            "type": "pad",
            "count": 1,
            "value": "<ROOT>"
          }
        ],
        "config": {
          "dim": 100,
          "word_dropout": 0.33,
          "add_group": "words",
          "initializer": {
            "zero_init": true
          }
        }
      },
      {
        "name": "pos",
        "key": "pos",
        "threshold": 2,
        "config": {
          "dim": 100,
          "word_dropout": 0.33,
          "initializer": {
            "zero_init": true
          }
        },
        "padding_funcs": [
          {
            "type": "pad",
            "count": 1,
            "value": "<ROOT>"
          }
        ]
      }
    ]
  }
