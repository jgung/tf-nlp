{
  "reader": "tsv",
  "max_epochs": 3,
  "checkpoint_epochs": 0.5,
  "batch_size": 8,
  "optimizer": {
    "name": "bert",
    "lr": {
      "name": "bert",
      "rate": 0.00001,
      "warmup_proportion": 0.1
    },
    "clip": 1.0
  },
  "features": {
    "targets": [
      {
        "name": "gold",
        "key": "gold",
        "rank": 1,
        "unknown_word": "0",
        "pad_word": "0"
      }
    ],
    "inputs": [
      {
        "name": "bert",
        "model": "https://tfhub.dev/google/albert_large/3",
        "output_type": "pooled_output"
      }
    ]
  },
  "encoders": [
    {
      "name": "bert-dropout",
      "type": "identity",
      "inputs": [
        "bert"
      ],
      "dropout": 0.1
    }
  ],
  "heads": [
    {
      "encoder": "bert-dropout",
      "name": "gold",
      "type": "classifier",
      "metric": "Accuracy"
    }
  ]
}