{
  "reader": "tsv_file_key",
  "batch_size": 16,
  "max_epochs": 12,
  "optimizer": {
    "name": "bert",
    "lr": {
      "name": "bert",
      "rate": 0.00002,
      "warmup_proportion": 0.1
    },
    "clip": 1.0
  },
  "features": {
    "targets": [
      {
        "name": "senti",
        "key": "senti",
        "rank": 1,
        "unknown_word": "2",
        "pad_word": "2",
        "default_val": "2"
      },
      {
        "name": "sent",
        "key": "sent",
        "rank": 1,
        "unknown_word": "Declarative",
        "pad_word": "Declarative",
        "default_val": "Declarative"
      },
      {
        "name": "qc",
        "key": "qc",
        "rank": 1,
        "unknown_word": "ENTY",
        "pad_word": "ENTY",
        "default_val": "ENTY"
      },
      {
        "name": "psa",
        "key": "psa",
        "rank": 1,
        "unknown_word": "NEUTRAL",
        "pad_word": "NEUTRAL",
        "default_val": "NEUTRAL"
      },
      {
        "name": "problem",
        "key": "problem",
        "rank": 1,
        "unknown_word": "NONE",
        "pad_word": "NONE",
        "default_val": "NONE"
      },
      {
        "name": "ap",
        "key": "ap",
        "rank": 1,
        "unknown_word": "OTHER",
        "pad_word": "OTHER",
        "default_val": "OTHER"
      }
    ],
    "inputs": [
      {
        "name": "bert",
        "drop_subtokens": false,
        "output_type": "cls",
        "model": "https://tfhub.dev/google/albert_large/1"
      }
    ]
  },
  "encoders": [
   {
      "name": "identity",
      "type": "identity",
      "dropout": 0.0,
      "inputs": [
        "bert"
      ]
    }
  ],
  "heads": [
    {
      "encoder": "identity",
      "name": "senti",
      "type": "classifier",
      "metric": "Accuracy"
    },
    {
      "encoder": "identity",
      "name": "sent",
      "type": "classifier",
      "metric": "Accuracy"
    },
    {
      "encoder": "identity",
      "name": "qc",
      "type": "classifier",
      "metric": "Accuracy"
    },
    {
      "encoder": "identity",
      "name": "psa",
      "type": "classifier",
      "metric": "Accuracy"
    },
    {
      "encoder": "identity",
      "name": "problem",
      "type": "classifier",
      "metric": "Accuracy"
    },
    {
      "encoder": "identity",
      "name": "ap",
      "type": "classifier",
      "metric": "Accuracy"
    }
  ]
}